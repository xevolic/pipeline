{
  "name": "wiosna-dev/pipeline",
  "type": "library",
  "license": "MIT",
  "description": "Sample project to test GitHub Action workflow and pipelines",
  "authors": [
    {
      "name": "Tomasz Kuter",
      "email": "tomasz.kuter@wiosna.org.pl"
    }
  ],
  "require": {
    "php": "^8.0",
    "psr/log": "^1|^2|^3",
    "symfony/config": "^6.0",
    "symfony/dependency-injection": "^6.0",
    "symfony/event-dispatcher": "^6.0",
    "symfony/http-foundation": "^6.0",
    "symfony/http-kernel": "^6.0",
    "symfony/security-core": "^6.0"
  },
  "require-dev": {
    "escapestudios/symfony2-coding-standard": "^3.12",
    "phpmd/phpmd": "^2.12",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^1.8",
    "phpunit/phpunit": "^9.5",
    "slevomat/coding-standard": "^8.2",
    "squizlabs/php_codesniffer": "^3.7",
    "vimeo/psalm": "^4.24"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "Wiosna\\Pipleline\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wiosna\\Pipleline\\Tests\\": "tests/"
    }
  }
}
